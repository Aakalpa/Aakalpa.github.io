  </span><span class="n">group_by</span><span class="p">(</span><span class="n">NumberOfSexualPartners</span><span class="p">)</span><span class="w"> </span><span class="o">%&gt;%</span><span class="w">
  </span><span class="n">ggplot</span><span class="p">(</span><span class="n">aes</span><span class="p">(</span><span class="n">x</span><span class="o">=</span><span class="n">NumberOfSexualPartners</span><span class="p">,</span><span class="w"> </span><span class="n">y</span><span class="o">=</span><span class="n">n</span><span class="p">,</span><span class="n">fill</span><span class="o">=</span><span class="n">Cancer</span><span class="p">))</span><span class="w"> </span><span class="o">+</span><span class="w">
