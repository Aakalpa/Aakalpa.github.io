  </span><span class="n">mutate</span><span class="p">(</span><span class="n">predicted</span><span class="w"> </span><span class="o">=</span><span class="w"> </span><span class="n">predicted</span><span class="o">&gt;</span><span class="m">0</span><span class="p">)</span><span class="w">

